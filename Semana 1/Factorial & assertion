"""En este archivo debe escribirse las solución requerida."""


def factorial(num):
    """Función que calcula el factorial de un entero dado como parámetro."""
    assert type(num) == int, "Invalid data"
    assert num >= 0, "Invalid data"

    result = 1

    for i in range(1, num + 1):
        result *= i

    return result


if __name__ == "__main__":
    print(factorial(5))
