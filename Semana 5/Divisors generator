"""MÃ³dulo que contiene un generador."""


def divisors(stop):
    """Generador de divisores de un entero positivo."""
    current = 0
    divs = [i for i in range(1, stop + 1) if stop % i == 0]

    while current <= len(divs) - 1:
        yield divs[current]
        current += 1


if __name__ == "__main__":
    for div in divisors(12):
        print(div)
